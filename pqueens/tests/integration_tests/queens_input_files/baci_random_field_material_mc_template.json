{
  "experiment_name": "baci_write_random_field_to_dat",
  "restart": false,
  "database": {
    "address": "localhost:27017"
  },
  "method": {
    "method_name": "monte_carlo",
    "method_options": {
      "seed": 1,
      "num_samples": 3,
      "model": "model",
      "result_description": {
        "write_results": true,
        "plot_results": false
      }
    }
  },
  "external_geometry": {
    "type": "baci_dat",
    "list_geometric_sets": [
      "DSURFACE 1"
    ],
    "associated_material_numbers_geometric_set": [
      [
        10,
        11
      ]
    ],
    "queens_tmp_dir": "{experiment_dir}"
  },
  "model": {
    "type": "simulation_model",
    "interface": "interface",
    "parameters": "parameters"
  },
  "interface": {
    "type": "job_interface",
    "resources": "resources",
    "driver": "driver"
  },
  "driver": {
    "driver_type": "baci",
    "driver_params": {
      "input_template": "{baci_input}",
      "path_to_executable": "{baci-release}",
      "path_to_postprocessor": "{post_drt_monitor}",
      "post_process_options": [
        "--field=structure --node=81 --start=1"
      ],
      "post_file_name_prefix_lst": [
        "baci_write_random_field_to_dat"
      ],
      "post_post": {
        "post_post_approach_sel": "baci",
        "delete_field_data": false,
        "target_time_lst": [
          "last"
        ],
        "post_post_file_name_prefix_lst": [
          "baci_write_random_field_to_dat.mon"
        ],
        "use_col_lst": [
          [
            1,
            3
          ]
        ],
        "skip_rows_lst": [
          4
        ]
      }
    }
  },
  "resources": {
    "local_machine": {
      "scheduler": "local_scheduler",
      "max-concurrent": 1,
      "max-finished-jobs": 10
    }
  },
  "local_scheduler": {
    "scheduler_type": "standard",
    "experiment_dir": "{experiment_dir}",
    "num_procs": 1,
    "num_procs_post": 1,
    "singularity": false
  },
  "parameters": {
    "random_variables": {},
    "random_fields": {
      "mat_param": {
        "type": "FLOAT",
        "external_definition": {
          "type": "material",
          "external_instance": "DSURFACE 1"
        },
        "dimension": 2,
        "min": 0,
        "max": 1,
        "corrstruct": "generic_external_random_field",
        "corr_length": 5.0,
        "std_hyperparam_rf": 0.03,
        "mean_fun_type": "constant",
        "mean_fun_params": [
          0.25
        ]
      }
    }
  }
}