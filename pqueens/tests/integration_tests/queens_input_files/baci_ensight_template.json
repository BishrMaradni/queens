{
  "experiment_name": "baci_ensight",
  "database": {
    "address": "localhost:27017",
    "drop_all_existing_dbs": true
  },
  "method": {
    "method_name": "monte_carlo",
    "method_options": {
      "seed": 42,
      "num_samples": 5,
      "model": "model",
      "result_description": {
        "write_results": true,
        "plot_results": false
      }
    }
  },
  "external_geometry":{
    "type": "baci_dat",
    "list_geometric_sets": ["DSURFACE 4"],
    "queens_tmp_dir": "{experiment_dir}"
  },
  "model": {
    "type": "simulation_model",
    "interface": "interface",
    "parameters": "parameters"
  },
  "interface": {
    "type": "job_interface",
    "resources": "resources",
    "driver": "driver"
  },
  "driver": {
    "driver_type": "baci",
    "driver_params": {
      "input_template": "{baci_input}",
      "path_to_executable": "{baci-release}",
      "path_to_postprocessor": "{post_drt_ensight}",
      "post_process_options": [
        ""
      ],
      "post_file_name_prefix_lst": [
        "baci_ensight"
      ],
      "post_post": {
        "post_post_approach_sel": "baci_ensight",
        "delete_field_data": false,
        "post_post_file_name_prefix_lst": [
          "baci_ensight"
        ],
        "field_specification": {
          "geometric_target": [
            [
              "geometric_set",
              "DSURFACE 4"
            ]
          ],
          "physical_field_dict": {
            "vtk_field_type": ["structure"],
            "vtk_array_type": ["point_array"],
            "vtk_field_label": ["displacement"],
            "field_components": [[0,1]]
          },
          "target_time": [["last"]]
        }
      }
    }
  },
  "resources": {
    "local_machine": {
      "scheduler": "local_scheduler",
      "max-concurrent": 1,
      "max-finished-jobs": 10
    }
  },
  "local_scheduler": {
    "restart": false,
    "scheduler_type": "standard",
    "experiment_dir": "{experiment_dir}",
    "num_procs": 1,
    "num_procs_post": 1
  },
  "parameters": {
    "random_variables": {
      "nue": {
        "type": "FLOAT",
        "size": 1,
        "min": 0.45,
        "max": 0.49,
        "distribution": "uniform",
        "distribution_parameter": [
          0.45,
          0.49
        ]
      },
      "pressure": {
        "type": "FLOAT",
        "size": 1,
        "min": 28000,
        "max": 38000,
        "distribution": "uniform",
        "distribution_parameter": [
          28000,
          38000
        ]
      }
    }
  }
}