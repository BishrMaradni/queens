        "distribution_parameter" : [-2.0,2.0]
      },
      "x2" : {
        "type" : "FLOAT",
        "size" : 1,
        "distribution" : "uniform",
        "distribution_parameter" : [-2.0,2.0]
      },
      "x3" : {
        "type" : "FLOAT",
        "size" : 1,
        "distribution" : "uniform",
        "distribution_parameter" : [-2.0,2.0]
      },
      "x4" : {
        "type" : "FLOAT",
        "size" : 1,
        "distribution" : "uniform",
        "distribution_parameter" : [-2.0,2.0]
      }
    }
  }
