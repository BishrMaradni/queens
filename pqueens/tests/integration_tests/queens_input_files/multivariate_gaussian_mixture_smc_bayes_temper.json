{
  "experiment_name" : "xxx",
  "database"  :{
    "address"   : "localhost:27017"
  },
  "method": {
    "method_name": "smc",
    "method_options" : {
      "seed" : 42,
      "num_particles" : 15,
      "model" : "model",
      "temper_type" : "bayes",
      "plot_trace_every" : 0,
      "result_description" : {
          "write_results" : true,
          "plot_results" : false,
          "cov" : true
      }
    }
  },
  "MCMC_Kernel": {
    "method_name": "metropolis-hastings",
    "method_options" : {
      "as_mcmc_kernel" : true,
      "num_chains" : 15,
      "num_samples" : 2,
      "proposal_distribution" : "normal_proposal"
    }
  },
  "model" : {
    "type" : "likelihood_model",
    "subtype": "gaussian_static",
    "forward_model": "forward_model",
    "output_label": "y_obs",
    "coordinate_labels": [],
    "likelihood_noise_type": "fixed",
    "fixed_likelihood_noise_value": 1.0,
    "nugget_noise_var": 1E-5,
    "experimental_csv_data_base_dirs": ["{experimental_data_path}"],
    "parameters" : "parameters"
  },
  "forward_model":{
    "type": "simulation_model",
    "interface" : "interface",
    "parameters" : "parameters"
  },
  "normal_proposal" : {
    "type" : "normal",
    "mean" : [0.0, 0.0, 0.0, 0.0],
    "cov" : [[0.001, 0.00, 0.00, 0.00],
