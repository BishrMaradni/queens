{
  "experiment_name" : "bmfmc_currin88",
  "database"  :{
    "address"   : "localhost:27017"
  },
  "method": {
    "method_name": "bmfmc",
    "method_options" :{
      "model": "joint_density_approx",
      "predictive_var": false,
      "BMFMC_reference": false,
      "y_pdf_support_min": -0.5,
      "y_pdf_support_max": 15.0,
      "result_description" : {
        "write_results" : true,
        "plotting_options": {
          "plot_booleans": [false, false, false],
          "plotting_dir": "{plot_dir}",
          "plot_names": ["pdfs.eps","manifold.eps", "ranking.eps"],
          "save_bool": [false, false, false],
          "animation_bool": false
        }
      }
    },
   "initial_design":{
     "num_HF_eval" : 100,
     "num_bins" : 20,
     "method": "{design_method}",
     "seed": 1
   }
  },
  "joint_density_approx": {
    "path_to_lf_mc_data" : ["{lf_mc_pickle_file}"],
    "high_fidelity_model": "high_fidelity_model",
    "path_to_hf_mc_reference_data" : null,
    "type" : "gp_approximation_gpy",
    "features_config" : "opt_features",
    "num_features": 1
    },
  "high_fidelity_model": {
    "type": "simulation_model",
    "interface": "interface",
    "parameters": "parameters"
  },
  "interface" : {
  "type" : "direct_python_interface",
  "main_file": "currin88_hifi.py"
  },
  "parameters": {
    "random_variables": {
      "x1": {
        "type": "FLOAT",
        "size": 1,
        "min": 0.0,
        "max": 1.0,
        "distribution": "uniform",
        "distribution_parameter": [0.0, 1.0]
      },
      "x2": {
        "type": "FLOAT",
        "size": 1,
        "min": 0.0,
        "max": 1.0,
        "distribution": "uniform",
        "distribution_parameter": [0.0, 1.0]
      }
    }
  }
}

