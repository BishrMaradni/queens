{
  "experiment_name": "xxx",
  "database": {
    "address": "localhost:27017"
  },
  "method": {
    "method_name": "monte_carlo",
    "method_options": {
      "seed": 44,
      "num_samples": 10,
      "model": "gp_surrogate_model",
      "result_description": {
        "write_results": true,
        "plot_results": false,
        "bayesian": false,
        "num_support_points": 10,
        "estimate_all": false
      }
    }
  },
  "gp_surrogate_model": {
    "type": "datafit_surrogate_model",
    "interface": "approximation_interface",
    "parameters": "parameters",
    "subordinate_model": "truth_model",
    "subordinate_iterator": "truth_model_iterator",
    "plotting_options": {
      "plot_booleans": [
        false,
        false
      ],
      "plotting_dir": "dummy",
      "plot_names": [
        "1D",
        "2D"
      ],
      "save_bool": [
        false,
        false
      ]
    }
  },
  "approximation_interface": {
    "type": "approximation_interface",
    "approximation": "gp_regression"
  },
  "gp_regression": {
    "type": "gp_precompiled",
    "stochastic_optimizer": "Adam",
    "learning_rate": 0.01,
    "optimization_type": "max",
    "max_iterations": 1000,
    "rel_L1_change_threshold": 1E-4,
    "rel_L2_change_threshold": 1E-4,
    "sigma_0_sq": 5.0,
    "l_scale_sq": 15.0,
    "sigma_n_sq": 0.1,
    "noise_var_lb": 0.000001,
    "data_scaling": {
      "type": "standard_scaler"
    },
    "hyper_prior_lengthscale": {
      "hyper_prior_bool": false,
      "f_S": 0.15,
      "f_M": 0.25
    }
  },
  "truth_model_iterator": {
    "method_name": "monte_carlo",
    "method_options": {
      "seed": 42,
      "num_samples": 20
    }
  },
  "truth_model": {
    "type": "simulation_model",
    "interface": "interface",
    "parameters": "parameters"
  },
  "interface": {
    "type": "direct_python_interface",
    "main_file": "branin_hifi.py"
  },
  "parameters": {
    "random_variables": {
      "x1": {
        "type": "FLOAT",
        "size": 1,
        "min": -5,
        "max": 10,
        "distribution": "uniform",
        "distribution_parameter": [
          -5,
          10
        ]
      },
      "x2": {
        "type": "FLOAT",
        "size": 1,
        "min": 0,
        "max": 15,
        "distribution": "uniform",
        "distribution_parameter": [
          0,
          15
        ]
      }
    }
  }
}