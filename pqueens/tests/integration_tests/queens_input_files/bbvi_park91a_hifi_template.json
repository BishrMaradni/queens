{
  "experiment_name": "inverse_bbvi_park91a_hifi",
  "database": {
      "address": "localhost:27017",
      "drop_existing": true
  },
  "method": {
      "method_name": "bbvi",
      "method_options": {
          "model": "model",
          "max_feval": 100000.0,
          "min_relative_change_variational_params": 0.001,
          "n_samples_per_iter": 20,
          "learning_rate": 0.07,
          "variational_approximation_type": "mean_field",
          "variational_family": "normal",
          "variational_transformation": null,
          "random_seed": 1,
          "result_description": {
              "write_results": true,
              "plotting_options": {
                  "plot_booleans": [false, false],
                  "plotting_dir": "{plot_dir}",
                  "plot_names": ["pdfs_params.eps", "variat_params_convergence.eps"],
                  "save_bool": [false, false],
                  "MLE_comparison": [0.5, 0.2]
              }
          }
      }
  },
  "model": {
    "type" : "likelihood_model",
    "subtype": "gaussian_static",
    "forward_model": "forward_model",
    "output_label": "y_obs",
    "coordinate_labels": ["x3", "x4"],
    "likelihood_noise_type": "jeffreys_prior",
    "nugget_noise_var": 1E-5,
    "experimental_csv_data_base_dirs": ["{experimental_data_path}"],
    "parameters" : "parameters"
  },
  "forward_model":{
    "type": "simulation_model",
    "interface" : "interface",
    "parameters" : "parameters"
  },
  "interface": {
      "type": "direct_python_interface",
      "main_file": "park91a_hifi_coords.py"
  },
  "parameters": {
      "random_variables": {
          "x1": {
              "type": "FLOAT",
              "distribution": "normal",
              "distribution_parameter": [0.6, 0.2],
              "size": 1
          },
          "x2": {
              "type": "FLOAT",
              "distribution": "normal",
              "distribution_parameter": [0.3, 0.1],
              "size": 1
          }
      }
  }
}
