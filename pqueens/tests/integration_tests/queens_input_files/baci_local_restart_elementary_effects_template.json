{
    "experiment_name": "{experiment_name}",
    "restart": true,
    "database": {
        "type": "mongodb",
        "address": "localhost:27017",
        "reset_existing_db": {drop_database_boolean}
    },
    "method": {
        "method_name": "elementary_effects",
        "method_options": {
            "seed": 2,
            "model": "model",
            "num_trajectories": 15,
            "num_optimal_trajectories": 5,
            "number_of_levels": 2,
            "confidence_level": 0.95,
            "local_optimization": true,
            "num_bootstrap_samples": 15,
            "result_description": {
                "write_results": true,
                "plotting_options": {
                    "plot_booleans": [
                        false,
                        false
                    ],
                    "plotting_dir": "dummy",
                    "plot_names": [
                        "bars",
                        "scatter"
                    ],
                    "save_bool": [
                        false,
                        false
                    ]
                }
            }
        }
    },
    "model": {
        "type": "simulation_model",
        "interface": "interface"
    },
    "interface": {
        "type": "job_interface",
        "resources": "resources",
        "driver": "driver"
    },
    "driver": {
        "driver_type": "baci",
        "driver_params": {
            "input_template": "{baci_input}",
            "path_to_executable": "{baci-release}",
            "path_to_postprocessor": "{post_drt_monitor}",
            "post_file_name_prefix_lst": [
                "QoI"
            ],
            "post_process_options": [
                "--field=structure --node=26 --start=1"
            ],
            "data_processor": "data_processor_baci"
        }
    },
    "data_processor_baci": {
        "type": "csv",
        "file_name_identifier": "QoI*",
        "file_options_dict": {
            "delete_field_data": false,
            "use_cols_lst": [1, 3],
            "index_column": 0,
            "skip_rows": 4,
            "filter": {
                "type": "by_target_values",
                "target_values": [5e-1],
                "tolerance": 1e-2
            }
        }
    },
    "resources": {
        "local_machine": {
            "scheduler": "local_scheduler",
            "max-concurrent": 1,
            "max-finished-jobs": 1000
        }
    },
    "local_scheduler": {
        "experiment_dir": "{experiment_dir}",
        "scheduler_type": "standard",
        "singularity": {singularity_boolean},
        "num_procs": 1,
        "num_procs_post": 1
    },
    "parameters": {
        "random_variables": {
            "nue": {
                "type": "FLOAT",
                "dimension": 1,
                "lower_bound": 0.45,
                "upper_bound": 0.49
            },
            "pressure": {
                "type": "FLOAT",
                "dimension": 1,
                "lower_bound": 28000,
                "upper_bound": 38000
            }
        }
    }
}
