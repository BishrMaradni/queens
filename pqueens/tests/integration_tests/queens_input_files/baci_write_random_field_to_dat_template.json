{
  "experiment_name" : "baci_write_random_field_to_dat",
  "database"  :{
    "address" : "localhost:27017",
    "drop_all_existing_dbs" : false
  },
  "method": {
    "method_name": "monte_carlo",
    "method_options" :{
      "seed" : 1,
      "num_samples" : 3,
      "model" : "model",
      "result_description" : {
          "write_results" : true,
          "plot_results" : false
      }
    }
  },
  "external_geometry": {
    "type": "baci_dat",
    "list_geometric_sets": ["DSURFACE 4"]
  },
  "model" : {
    "type" : "simulation_model",
    "interface" : "interface",
    "parameters" : "parameters"
  },
  "interface" : {
    "type" : "job_interface",
    "resources": "resources",
    "driver" : "driver"
  },
  "driver" :{
    "driver_type"           : "baci",
    "driver_params"         :{
      "input_template"        : "{baci_input}",
      "path_to_executable"    : "{baci-release}",
      "path_to_postprocessor" : "{post_drt_monitor}",
      "post_process_options"  : ["--field=structure --node=26 --start=1"],
      "post_file_name_prefix_lst": ["QoI"],
      "post_post": {
          "post_post_approach_sel": "baci",
          "delete_field_data": false,
          "target_time_lst": [5e-1],
          "time_tol_lst": [1e-2],
          "post_post_file_name_prefix_lst": ["QoI"],
          "use_col_lst": [[1, 3]],
          "skip_rows_lst": [4]
      }
    }
  },
  "resources" : {
    "local_machine" : {
      "scheduler"         : "local_scheduler",
      "max-concurrent"    : 1,
      "max-finished-jobs" : 10001
    }
  },
  "local_scheduler" : {
    "restart": false,
    "scheduler_type": "standard",
    "experiment_dir": "{experiment_dir}",
    "num_procs" : 1,
    "num_procs_post": 1
  },
  "parameters" : {
    "random_variables" : {
      "emodul" : {
        "type": "FLOAT",
        "size": 1,
        "distribution": "lognormal",
        "distribution_parameter": [
          6.392,
          0.0705
        ]
      },
      "nue": {
        "type": "FLOAT",
        "size": 1,
        "distribution": "uniform",
        "distribution_parameter": [
          0.45,
          0.49
          ]
      },
      "pressure": {
        "type": "FLOAT",
        "size": 1,
        "distribution": "uniform",
        "distribution_parameter": [
          28000,
          38000
          ]
      }
    },
    "random_fields" : {
      "random_inflow" : {
        "type" : "FLOAT",
        "external_definition": {
          "type": "dirichlet",
          "external_instance": "DSURFACE 4",
          "dof_for_field": 3,
          "funct_for_field": 0,
          "dofs_deterministic": [1, 2],
          "value_dofs_deterministic": [0, 0],
          "funct_for_deterministic_dofs": [0, 0]
        },
        "dimension" :  1,
        "min"  : 0,
        "max"  : 1,
        "corrstruct" : "generic_external_random_field",
        "corr_length": 1.0,
        "std_hyperparam_rf": 0.001,
        "mean_fun_type": "constant",
        "mean_fun_params": [0]
    }
  }
  }}
