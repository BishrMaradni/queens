{
  "experiment_name": "xxx",
  "database": {
    "type": "mongodb",
    "address": "localhost:27017"
  },
  "method": {
    "method_name": "monte_carlo",
    "method_options": {
      "seed": 44,
      "num_samples": 10,
      "model": "gp_surrogate_model",
      "result_description": {
        "write_results": true,
        "plot_results": false,
        "bayesian": false,
        "num_support_points": 10,
        "estimate_all": false
      }
    }
  },
  "gp_surrogate_model": {
    "type": "datafit_surrogate_model",
    "interface": "approximation_interface",
    "parameters": "parameters",
    "subordinate_model": "truth_model",
    "subordinate_iterator": "truth_model_iterator",
    "plotting_options": {
      "plot_booleans": [
        false,
        false
      ],
      "plotting_dir": "dummy",
      "plot_names": [
        "1D",
        "2D"
      ],
      "save_bool": [
        false,
        false
      ]
    }
  },
  "approximation_interface": {
    "type": "approximation_interface",
    "approximation": "gp_regression"
  },
  "gp_regression": {
    "type": "gp_approximation_gpflow",
    "train_likelihood_variance": false,
    "number_restarts": 5,
    "number_training_iterations": 1000,
    "dimension_lengthscales": 2
  },
  "truth_model_iterator": {
    "method_name": "monte_carlo",
    "method_options": {
      "seed": 42,
      "num_samples": 20
    }
  },
  "truth_model": {
    "type": "simulation_model",
    "interface": "interface",
    "parameters": "parameters"
  },
  "interface": {
    "type": "direct_python_interface",
    "main_file": "branin_hifi.py"
  },
  "parameters": {
    "random_variables": {
      "x1": {
        "type": "FLOAT",
        "size": 1,
        "min": -5,
        "max": 10,
        "distribution": "uniform",
        "distribution_parameter": [
          -5,
          10
        ]
      },
      "x2": {
        "type": "FLOAT",
        "size": 1,
        "min": 0,
        "max": 15,
        "distribution": "uniform",
        "distribution_parameter": [
          0,
          15
        ]
      }
    }
  }
}