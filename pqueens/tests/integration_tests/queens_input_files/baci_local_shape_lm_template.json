{
  "experiment_name": "{experiment_name}",
  "database": {
    "type": "mongodb",
    "address": "localhost:27017"
  },
  "method": {
    "method_name": "baci_lm",
    "method_options": {
      "model": "model",
      "jac_method": "2-point",
      "jac_rel_step": 1e-3,
      "jac_abs_step": 1e-5,
      "max_feval": 50,
      "init_reg": 1,
      "update_reg": "grad",
      "algorithm": "LSQ",
      "convergence_tolerance": 1.0e-2,
      "initial_guess": [
        1500,
        0.4
      ],
      "result_description": {
        "write_results": true,
        "plot_results": true
      },
      "bounds": [
        [
          0,
          -0.5
        ],
        [Infinity,
          0.49
        ]
      ]
    }
  },
  "model": {
    "type": "simulation_model",
    "interface": "interface"
  },
  "interface": {
    "type": "job_interface",
    "resources": "resources",
    "driver": "driver"
  },
  "driver": {
    "driver_type": "mpi",
    "input_template": "{baci_input}",
    "path_to_executable": "{baci-release}",
    "path_to_postprocessor": "{post_processor}",
    "post_process_options": "--filter=ensight",
    "post_file_prefix": "xx",
    "data_processor": "data_processor_baci"
  },
  "data_processor_baci": {
    "type": "ensight_interface_discrepancy",
    "file_name_identifier": "xx*structure.case",
    "file_options_dict": {
      "path_to_ref_data": "{baci_input_monitor}",
      "delete_field_data": false,
      "time_tol": 1e-2,
      "displacement_fields": [
        "displacement"
      ],
      "problem_dimension": "2d",
      "visualization": false
    }
  },
  "resources": {
    "my-machine": {
      "scheduler": "scheduler",
      "max-concurrent": 3,
      "max-finished-jobs": 10
    }
  },
  "parameters": {
    "random_variables": {
      "E": {
        "type": "FLOAT",
        "dimension": 1
      },
      "nue": {
        "type": "FLOAT",
        "dimension": 1
      }
    }
  },
  "scheduler": {
    "scheduler_type": "standard",
    "num_procs": 2,
    "singularity": {singularity_boolean}
  }
}
