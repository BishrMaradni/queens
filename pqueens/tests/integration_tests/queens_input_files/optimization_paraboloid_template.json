{
  "experiment_name": "Paraboloid",
  "database": {
    "type": "mongodb",
    "address": "localhost:27017"
  },
  "method": {
    "method_name": "optimization",
    "method_options": {
      "model": "model",
      "initial_guess": [
        2.0,
        0.0
      ],
      "algorithm": "{algorithm}",
      "result_description": {
        "write_results": true,
        "plot_results": true
      },
      "bounds": [
        [
          0, Infinity
        ],
        [
          0, Infinity
        ]
      ],
      "constraints": {
        "cons1": {
          "type": "ineq",
          "fun": "lambda x:  x[0] - 2 * x[1] + 2"
        },
        "cons2": {
          "type": "ineq",
          "fun": "lambda x: -x[0] - 2 * x[1] + 6"
        },
        "cons3": {
          "type": "ineq",
          "fun": "lambda x: -x[0] + 2 * x[1] + 2"
        }
      }
    }
  },
  "model": {
    "type": "simulation_model",
    "interface": "interface",
    "parameters": "parameters"
  },
  "normal_proposal": {
    "type": "normal",
    "proposal_covariance": 1.0
  },
  "interface": {
    "type": "direct_python_interface",
    "main_file": "paraboloid.py"
  },
  "parameters": {
    "random_variables": {
      "x1": {
        "type": "FLOAT",
        "size": 1
      },
      "x2": {
        "type": "FLOAT",
        "size": 1
      }
    }
  }
}
