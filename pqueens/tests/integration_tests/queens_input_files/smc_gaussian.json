{
  "experiment_name": "xxx",
  "database": {
    "type": "mongodb",
    "address": "localhost:27017"
  },
  "method": {
    "method_name": "smc",
    "method_options": {
      "seed": 42,
      "num_particles": 10,
      "model": "model",
      "temper_type": "bayes",
      "plot_trace_every": 0,
      "result_description": {
        "write_results": true,
        "plot_results": true,
        "cov": false
      }
    }
  },
  "MCMC_Kernel": {
    "method_name": "metropolis-hastings",
    "method_options": {
      "as_mcmc_kernel": true,
      "num_chains": 10,
      "num_samples": 3,
      "proposal_distribution": "proposal_distribution"
    }
  },
  "model": {
    "type": "likelihood_model",
    "subtype": "gaussian_static",
    "forward_model": "forward_model",
    "output_label": "y_obs",
    "coordinate_labels": [],
    "likelihood_noise_type": "fixed",
    "fixed_likelihood_noise_value": 1.0,
    "nugget_noise_var": 1E-5,
    "post_post": {
      "post_file_name_identifier": "*.csv",
      "file_options_dict": {
        "header_row": 0,
        "index_column": false,
         "filter_format": "dict",
          "filter": {
              "type": "entire_file"
          }
        }
    },
    "experimental_csv_data_base_dir": "{experimental_data_path}",
    "parameters": "parameters"
  },
  "forward_model": {
    "type": "simulation_model",
    "interface": "interface",
    "parameters": "parameters"
  },
  "proposal_distribution": {
    "distribution": "normal",
    "mean": 0.0,
    "covariance": 1.0
  },
  "interface": {
    "type": "direct_python_interface",
    "main_file": "gaussian_logpdf.py"
  },
  "parameters": {
    "random_variables": {
      "x": {
        "size": 1,
        "distribution": "normal",
        "mean": 2.0,
        "covariance": 1.0
      }
    }
  }
}
