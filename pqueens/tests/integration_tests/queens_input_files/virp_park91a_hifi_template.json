{
  "experiment_name": "inverse_virp_park91a_hifi",
  "database": {
    "address": "localhost:27017",
    "drop_existing": true
  },
  "method": {
    "method_name": "virp",
    "method_options": {
      "model": "model",
      "max_feval": 1300,
      "min_relative_change_variational_params": -1,
      "num_iter_average_convergence": 5,
      "n_samples_per_iter": 2,
      "score_function_bool": true,
      "finite_difference_step": 1e-5,
      "variational_distribution": {
        "variational_approximation_type": "mean_field",
        "variational_family": "normal"
      },
      "optimization_options": {
        "learning_rate": 0.01,
        "gradient_clipping": true,
        "gradient_clipping_norm_threshold": 1e6,
        "natural_gradient": true,
        "FIM_dampening": true,
        "decay_start_iteration": 50,
        "dampening_coefficient": 1e-2,
        "FIM_dampening_lower_bound": 1e-8
      },
      "variational_transformation": null,
      "variational_parameter_initialization": "prior",
      "random_seed": 1,
      "result_description": {
        "write_results": true,
        "export_iteration_data": true,
        "plotting_options": {
          "plot_boolean": false,
          "plotting_dir": "{plot_dir}",
          "plot_name": "variational_params_convergence.eps",
          "save_bool": false
        }
      }
    }
  },
  "model": {
    "type": "likelihood_model",
    "subtype": "gaussian_static",
    "forward_model": "forward_model",
    "output_label": "y_obs",
    "coordinate_labels": [
      "x3",
      "x4"
    ],
    "likelihood_noise_type": "jeffreys_prior",
    "nugget_noise_var": 1E-8,
    "experimental_csv_data_base_dirs": [
      "{experimental_data_path}"
    ],
    "parameters": "parameters"
  },
  "forward_model": {
    "type": "simulation_model",
    "interface": "interface",
    "parameters": "parameters"
  },
  "interface": {
    "type": "direct_python_interface",
    "main_file": "park91a_hifi_coords.py"
  },
  "parameters": {
    "random_variables": {
      "x1": {
        "type": "FLOAT",
        "distribution": "normal",
        "distribution_parameter": [
          0.6,
          0.2
        ],
        "size": 1
      },
      "x2": {
        "type": "FLOAT",
        "distribution": "normal",
        "distribution_parameter": [
          0.3,
          0.1
        ],
        "size": 1
      }
    }
  }
}