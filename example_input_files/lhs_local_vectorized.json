{
  "experiment_name" : "ee_invaaa_local",
  "database"  :{
    "address"   : "localhost:27017",
    "drop_existing" : "True"
  },
  "method": {
    "method_name": "lhs",
    "method_options" :{
      "seed" : 1,
      "num_iterations": 10,
      "num_samples": 20,
      "model" : "model",
      "result_description" : {
        "write_results" : true,
        "plot_results" : true
      }
    }
  },
  "model" : {
    "type" : "simulation_model",
    "interface" : "interface",
    "parameters" : "parameters"
  },
  "interface" : {
    "type" : "job_interface",
    "resources" : "resources",
    "driver" : "driver"
  },
  "driver" :{
    "driver_type"           : "baci_native",
    "driver_params"         :{
      "input_template"        : "/home/nitzler/workspace/BACI_Rep/input/invaaa_ee.dat",
      "path_to_executable"    : "/home/nitzler/workspace/BACI_Rep/Build/baci-release",
      "path_to_postprocessor" : "/home/nitzler/workspace/BACI_Rep/Build/post_drt_monitor",
      "cluster_bind"          : "/opt:/opt,/bin:/bin,/etc:/etc,/lib:/lib,/lib64:/lib64,/lnm:/lnm",
      "no_singularity": true,
      "post_process_options"  : ["--field=structure --node=26 --start=1"],
      "experiment_dir"        : "/home/nitzler/workspace/BACI_Rep/output/elementary_effects_demo",
      "post_post"         : {
        "post_post_approach_sel": "baci_vector",
        "delete_field_data" : false,
        "target_time": 5e-1,
        "time_tol": 1e-2,
        "file_prefix": "QoI",
        "usecols": [3],
        "skiprows":4}
    }
  },
  "resources" : {
    "local_machine" : {
      "scheduler"         : "local_scheduler",
      "max-concurrent"    : 4,
      "max-finished-jobs" : 1000
    }
  },
  "local_scheduler" : {
    "scheduler_type" : "local",
    "num_procs" : 1,
    "num_procs_post": 1,
    "walltime" : "120:00:00",
    "meta_output" : "True"
  },
  "parameters" : {
    "random_variables" :{
    "youngs1" : {
      "type" : "FLOAT",
      "distribution": "uniform",
      "size" : 1,
      "distribution_parameter": [0.8E6,1.2E6]
      },
    "youngs2" : {
      "type" : "FLOAT",
      "distribution": "uniform",
      "size" : 1,
      "distribution_parameter": [0.6E6,0.9E6]
      },
      "nue" : {
      "type" : "FLOAT",
      "distribution": "uniform",
      "size" : 1,
      "distribution_parameter": [0.45,0.49]
      },
      "beta" : {
      "type" : "FLOAT",
      "distribution": "uniform",
      "size" : 1,
      "distribution_parameter": [1.7E6,2.1E6]
      },
      "pressure" : {
      "type" : "FLOAT",
      "distribution": "uniform",
      "size" : 1,
      "distribution_parameter": [28000,38000]
      }
    }
      }
}
