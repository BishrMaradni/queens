  "parameters" : {
    "random_variables" :{
    "lateral_position" : {
      "type" : "FLOAT",
      "size" : 1,
      "min"  : 0.8E6,
      "max"  : 1.2E6
      },
    "diameter" : {
      "type" : "FLOAT",
      "size" : 1,
      "min"  : 0.6E6,
      "max"  : 0.9E6
      },
      "viscosity" : {
      "type" : "FLOAT",
      "size" : 1,
      "min"  : 0.45,
      "max"  : 0.49
      }
    }
      }
