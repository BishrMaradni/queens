{
  "experiment_name" : "MC_cylinder",
  "database"  :{
    "address"   : "localhost:27017",
    "drop_existing" : "True"
  },
  "method": {
    "method_name": "monte_carlo",
    "method_options" :{
      "seed" : 1,
      "num_samples" : 5000,
      "model" : "model",
      "result_description" : {
          "write_results" : true,
          "plot_results" : false
      }
    }
  },
  "model" : {
    "type" : "simulation_model",
    "interface" : "interface",
    "parameters" : "parameters"
  },
  "interface" : {
    "type" : "job_interface",
    "resources" : "resources",
    "driver" : "driver"
  },
  "driver" :{
    "driver_type"           : "dg_navier_stokes",
    "driver_params"         :{
      "input_template"        : "/home/nitzler/workspace/navierstokes/applications/incompressible_navier_stokes_test_cases/flow_past_cylinder.json",
      "path_to_executable"    : "/home/nitzler/workspace/navierstokes/applications/incompressible_navier_stokes/",
      "path_to_postprocessor" : "None",
      "experiment_dir"        : "/home/nitzler/workspace/navierstokes/applications/output/demo",
      "post_post"         : {
        "type" : "time_series",
        "file_name" : ["lift"],
        "time_discretization": 150,
        "delete_field_data" : "False",
        "target_time": [4,7],
        "time_tol": 0.02e-01,
        "usecols": [1,3],
        "skiprows":1}
    }
  },
  "resources" : {
    "local_machine" : {
      "scheduler"         : "local_scheduler",
      "max-concurrent"    : 1000,
      "max-finished-jobs" : 1000
    }
  },
  "local_scheduler" : {
    "scheduler_type" : "local",
